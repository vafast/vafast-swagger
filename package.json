{
	"name": "@vafast/swagger",
	"version": "0.0.2",
	"description": "Plugin for Vafast for Swagger/OpenAPI documentation",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"build": "tsup",
		"dev": "npx tsx watch example/index.ts",
		"test": "npx vitest run",
		"release": "npm run build && npm run test && npx bumpp && npm publish --access=public"
	},
	"peerDependencies": {
		"vafast": "^0.3.10"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"devDependencies": {
		"@types/bun": "^1.1.2",
		"@typescript-eslint/eslint-plugin": "^7.9.0",
		"@typescript-eslint/parser": "^7.9.0",
		"eslint": "^9.2.0",
		"rimraf": "^6.0.1",
		"tsup": "^8.5.1",
		"typescript": "^5.4.5",
		"vafast": "^0.3.10",
		"vitest": "^4.0.16"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"keywords": [
		"vafast",
		"swagger",
		"openapi"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/vafastjs/vafast-swagger"
	},
	"author": {
		"name": "saltyAom",
		"url": "https://github.com/SaltyAom",
		"email": "saltyaom@gmail.com"
	},
	"homepage": "https://github.com/vafastjs/vafast-swagger",
	"bugs": "https://github.com/vafastjs/vafast-swagger/issues"
}
